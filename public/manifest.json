{
  "name": "OneToc",
  "description": "Toc For Websites.",
  "version": "1.0.1",
  "manifest_version": 3,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "react.js",
        "react-dom.js",
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
